{
  "logger": {
    "Level": "Verbose",
    "Console": {
      "Enabled": "true"
    },
    "File": {
      "Enabled": "true",
      "Path": "/usr/bin/transaction-microservice/logs/balanceApi-.json",
      "Interval": "Day"
    },
    "MinimumLevelOverrides": {
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning"
    },
    "ExcludeProperties": [
    ]
  },
  "app": {
    "Service": "accounctSummary.Api",
    "Instance": "1",
    "Version": "1.0.0"
  },
  "metrics": {
    "Enabled": true,
    "InfluxEnabled": false,
    "PrometheusEnabled": true,
    "PrometheusFormatter": "",
    "Database": "",
    "Interval": 10,
    "Tags": {
      "app": "balance.api",
      "env": "Production",
      "server": "linux"

    }
  }
}